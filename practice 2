// practice function call

// function declaration
int add_two_ints(int a, int b);

int main(void)
{
// ask user for input 
printf("Give me an integer: ");
int x = GetInt();
printf("Give another integer: ");
int y = GetInt();

// add the two numbrrs together via a function call 
	int z = add_two_ints(x, y);

// output the result
	printf("the sum of %i and %i is %i\n, x, y, z);
}

int add_two_ints(int a , int b)
{
	int sum = a + b;
	return sum;
}
