#include <cs50.h>
#include <stdio.h>
#include <string.h>

bool compare_strings(string a, string b);

int main(void)
{
	// get a string
	string s = get_string("s: ");

	// copy strings address
	string t = s;

	// capitalize first letter 
	if (strlen(t) > 0)
	{
	t[0] = toupper(t[0]);
	}

	//print string twice
	printf("s: %s\n, s);
	printf("t: %s\n, t);
}
